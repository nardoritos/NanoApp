<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="NanoApp.App">
	<Application.Resources>
		<ResourceDictionary>
			<OnPlatform x:TypeArguments="x:String" x:Key="RobotoLight">
				<On Platform="Android" Value="Roboto-Light.ttf#Roboto Light" />
			</OnPlatform>
			<OnPlatform x:TypeArguments="x:String" x:Key="RobotoBold">
				<On Platform="Android" Value="Roboto-Bold.ttf#Roboto Bold" />
			</OnPlatform>
		</ResourceDictionary>
	</Application.Resources>
</Application>